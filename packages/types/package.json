{
  "name": "@moge/types",
  "version": "1.0.0",
  "description": "Shared types for Next.js and NestJS",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "tsc --watch --project tsconfig.json",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "catalog:"
  },
  "dependencies": {
    "zod": "catalog:"
  }
}
