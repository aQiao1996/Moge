{
  "name": "@moge/types",
  "version": "1.0.0",
  "description": "Shared types for Next.js and NestJS",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "tsc --watch --project tsconfig.json",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "catalog:"
  },
  "dependencies": {
    "zod": "catalog:"
  }
}
